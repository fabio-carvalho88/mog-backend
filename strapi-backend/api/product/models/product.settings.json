{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "markets": {
      "collection": "market",
      "via": "products"
    },
    "model": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "family": {
      "type": "enumeration",
      "enum": [
        "MAM4PRO",
        "mDeck",
        "Vizzi"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "shortDescription": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "longDescription": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "partNumber": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "datasheet": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "price": {
      "type": "biginteger",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "details": {
      "type": "component",
      "repeatable": true,
      "component": "product.details",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "explores": {
      "collection": "explore",
      "via": "products"
    },
    "services": {
      "collection": "service",
      "via": "products"
    }
  }
}
