{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "markets": {
      "collection": "market",
      "via": "products"
    },
    "model": {
      "type": "string"
    },
    "family": {
      "type": "enumeration",
      "enum": [
        "MAM4PRO",
        "mDeck",
        "Vizzi"
      ]
    },
    "shortDescription": {
      "type": "string"
    },
    "longDescription": {
      "type": "text"
    },
    "partNumber": {
      "type": "string"
    },
    "datasheet": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": false
    },
    "price": {
      "type": "biginteger"
    },
    "details": {
      "type": "component",
      "repeatable": true,
      "component": "product.details"
    },
    "explores": {
      "via": "products",
      "collection": "explore"
    },
    "services": {
      "via": "products",
      "collection": "service"
    }
  }
}
